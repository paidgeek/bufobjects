{% set fields = definition.fields %}
{% for p in definition.parentPath -%}
  {% set fields = merge(fields, p.fields) %}
{%- endfor -%}
{% for field in fields -%}
  {%- set snakeCase = utils.toSnakeCase(field.name) -%}
  {%- set className = utils.getClassName(field.type) -%}
  {%- if (className == "ArrayType") -%}
    dst.{{ snakeCase }}_ = std::array< {{ trim(macros.getBasePrimitiveType(field.type.type)) }}, {{ field.type.length }}>( {{ snakeCase }}_ );
  {%- elseif (className == "ListType") -%}
    dst.{{ snakeCase }}_ = std::vector< {{ trim(macros.getBaseType(field.type.type)) }} >({{ snakeCase }}_);
  {%- elseif (utils.getClassName(field.type.definition) == "EnumDefinition") -%}
    dst.{{ snakeCase }}_ = {{ snakeCase }}_;
  {%- elseif (field.type.token.toString() == "IDENTIFIER") -%}
    if({{ snakeCase }}_ != nullptr) {
      {{ snakeCase }}_.CopyTo(dst.{{ snakeCase }}_);
    }
  {%- else -%}
    dst.{{ snakeCase }}_ = {{ snakeCase }}_;
  {%- endif -%}
{%- endfor %}