{%- import "/cpp/macros.twig" as macros -%}

{% for field in definition.fields -%}
  {%- set snakeCase = utils.toSnakeCase(field.name) -%}
  {%- set className = utils.getClassName(field.type) -%}
  {%- set typeName = trim(macros.getTypeName(utils, field.type)) -%}

  {% if (utils.getClassName(field.type.definition) == "StructDefinition" or utils.getClassName(field.type.definition) == "EnumDefinition" or field.type.token.toString() != "IDENTIFIER") %}
    inline const {{ typeName }}& Get{{ field.name }}() const { return {{ snakeCase }}_; }
    inline {{ typeName }}& Get{{ field.name }}() { return {{ snakeCase }}_; }
    inline void Set{{ field.name }}(const {{ typeName }}& {{ snakeCase }}) { {{ snakeCase }}_ = {{ snakeCase }}; }
  {% else %}
    inline {{ typeName }} Get{{ field.name }}() { return {{ snakeCase }}_; cached_size_ = 0; }
    inline const {{ typeName }} Get{{ field.name }}() const { return {{ snakeCase }}_; }
    inline void Set{{ field.name }}({{ typeName }} {{ snakeCase }}) { {{ snakeCase }}_ = {{ snakeCase }}; cached_size_ = 0; }
  {% endif %}

  {% if (className == "VectorType") %}
    {% if (utils.getClassName(field.type.type.definition) == "StructDefinition" or field.type.type.token.toString() != "IDENTIFIER") %}
      inline const {{ trim(macros.getBaseNoPointer(utils, field.type.type))}}& Get{{ field.name }}(int index) const { return {{ snakeCase }}_[index]; }
      inline {{ trim(macros.getBaseNoPointer(utils, field.type.type))}}& Get{{ field.name }}(int index) { return {{ snakeCase }}_[index]; }
      inline void Set{{ field.name }}(int index, const {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }}& value) { {{ snakeCase }}_[index] = value; }
    {% else %}
      inline const {{ trim(macros.getBaseNoPointer(utils, field.type.type))}} Get{{ field.name }}(int index) const { return {{ snakeCase }}_[index]; }
      inline {{ trim(macros.getBaseNoPointer(utils, field.type.type))}} Get{{ field.name }}(int index) { return {{ snakeCase }}_[index]; cached_size_ = 0; }
      inline void Set{{ field.name }}(int index, {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }} value) { {{ snakeCase }}_[index] = value; cached_size_ = 0; }
    {% endif %}
  {% elseif (className == "ArrayType") %}
    {% if (utils.getClassName(field.type.type.definition) == "StructDefinition" or field.type.type.token.toString() != "IDENTIFIER") %}
      inline const {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }}& Get{{ field.name }}(int index) const { return {{ snakeCase }}_[index]; }
      inline {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }}& Get{{ field.name }}(int index) { return {{ snakeCase }}_[index]; }
      inline void Set{{ field.name }}(int index, const {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }}& value) { {{ snakeCase }}_[index] = value; }
    {% else %}
      inline const {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }} Get{{ field.name }}(int index) { return {{ snakeCase }}_[index]; }
      inline {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }} Get{{ field.name }}(int index) { return {{ snakeCase }}_[index]; cached_size_ = 0; }
      inline void Set{{ field.name }}(int index, {{ trim(macros.getBaseNoPointer(utils, field.type.type)) }} value) { {{ snakeCase }}_[index] = value; cached_size_ = 0; }
    {% endif %}
  {% elseif (className == "MapType") %}
    {%- set keyType = trim(macros.getBaseNoPointer(utils, field.type.keyType)) -%}
    {%- set valueType = trim(macros.getBaseNoPointer(utils, field.type.valueType)) -%}

    {%- if (utils.getClassName(field.type.valueType.definition) == "StructDefinition" or field.type.valueType.token.toString() != "IDENTIFIER") -%}
      {%- if (utils.getClassName(field.type.keyType.definition) == "StructDefinition" or field.type.keyType.token.toString() != "IDENTIFIER") -%}
        inline {{ valueType }}& Get{{ field.name }}(const {{ keyType }}& key) { return {{ snakeCase }}_[key]; }
        inline const {{ valueType }}& Get{{ field.name }}(const {{ keyType }}& key) const { return {{ snakeCase }}_[key]; }
        inline void Set{{ field.name }}(const {{ keyType }}& key, {{ valueType }}& value) { {{ snakeCase }}_[key] = value; }
      {%- else -%}
        inline const {{ valueType }} Get{{ field.name }}(const {{ keyType }}& key) const { return {{ snakeCase }}_[key]; }
        inline {{ valueType }} Get{{ field.name }}(const {{ keyType }}& key) { return {{ snakeCase }}_[key]; cached_size_ = 0; }
        inline void Set{{ field.name }}(const {{ keyType }}& key, {{ valueType }}& value) { {{ snakeCase }}_[key] = value; cached_size_ = 0; }
      {%- endif -%}
    {%- else -%}
      {%- if (utils.getClassName(field.type.keyType.definition) == "StructDefinition" or field.type.keyType.token.toString() != "IDENTIFIER") -%}
        inline const {{ valueType }}& Get{{ field.name }}(const {{ keyType }}& key) const { return {{ snakeCase }}_[key]; }
        inline {{ valueType }}& Get{{ field.name }}(const {{ keyType }}& key) { return {{ snakeCase }}_[key]; }
        inline void Set{{ field.name }}(const {{ keyType }}& key, {{ valueType }}& value) { {{ snakeCase }}_[key] = value; }
      {%- else -%}
        inline const {{ valueType }} Get{{ field.name }}(const {{ keyType }}& key) const { return {{ snakeCase }}_[key]; }
        inline {{ valueType }} Get{{ field.name }}(const {{ keyType }}& key) { return {{ snakeCase }}_[key]; cached_size_ = 0; }
        inline void Set{{ field.name }}(const {{ keyType }}& key, {{ valueType }} value) { {{ snakeCase }}_[key] = value; cached_size_ = 0; }
      {%- endif -%}
    {%- endif -%}

  {% endif %}

{%- endfor %}
