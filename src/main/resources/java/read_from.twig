{%- for field in definition.fields -%}
  {%- set typeName = trim(macros.getTypeName(utils, field.type)) -%}
  {%- set name = concat("this.", utils.toCamelCase(field.name)) -%}
  {%- set className = utils.getClassName(field.type) -%}
  {
  {%- if (className == "ArrayType") -%}
    {% set elementName = concat(name, "[i]") %}
    {% set baseType = trim(macros.getBasePrimitiveType(field.type)) %}
    if({{ name }} == null) {
      {{ name }} = new {{ baseType }}[{{ utils.toUpperCase(field.name) }}_LENGTH];
    }
    for(int i = 0; i < {{ utils.toUpperCase(field.name) }}_LENGTH; i++) {
    {{ trim(macros.readPrimary(utils, field.type.type, elementName)) }}
    }
  {%- elseif (className == "ListType") -%}
    int size = 0;
    {
    {% include concat("/java/read/u32.twig") with {name:"size"} %}
    }
    if({{ name }} == null) {
      {{ name }} = new java.util.ArrayList<>(size);
    }
    for(int i = 0; i < size; i++) {
    {{ trim(macros.getBaseType(field.type.type)) }} e = {{ name }}.get(i);
    {{ trim(macros.readPrimary(utils, field.type.type, "e")) }}
      {{ name }}.add(e);
    }
  {% else %}
    {{ trim(macros.readPrimary(utils, field.type, name)) }}
  {% endif %}
  }
{%- endfor -%}